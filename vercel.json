{
  "version": 2,
  "builds": [
    {
      "src": "web/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "web/main.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "web/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "web/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
